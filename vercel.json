{
    "functions":{
        "api/index.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "/api/index.php",
            "dest": "/api/index.php"
        },
        {
            "src": "/boostrap/css/(.*)",
            "dest": "/public/boostrap/css/$1"
        },
        {
            "src": "/boostrap/fonts/(.*)",
            "dest": "/public/boostrap/fonts/$1"
        },
        {
            "src": "/boostrap/js/(.*)",
            "dest": "/public/boostrap/js/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/$1"
        },
        {
            "src": "/fonts/(.*)",
            "dest": "/public/fonts/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/$1"
        },
        {
            "src": "/Linealcons/(.*)",
            "dest": "/public/Linealcons/$1"
        },
        {
            "src": "/Linealcons/fonts/(.*)",
            "dest": "/public/Linealcons/fonts/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        },
        {
            "src": "/images/AsciiArt/(.*)",
            "dest": "/public/images/AsciiArt/$1"
        },
        {
            "src": "/images/DungeonTravel/(.*)",
            "dest": "/public/images/DungeonTravel/$1"
        },
        {
            "src": "/images/DungeonTravel/Bombe/(.*)",
            "dest": "/public/images/DungeonTravel/Bombe/$1"
        },
        {
            "src": "/images/DungeonTravel/Box/(.*)",
            "dest": "/public/images/DungeonTravel/Box/$1"
        },
        {
            "src": "/images/DungeonTravel/Chaines/(.*)",
            "dest": "/public/images/DungeonTravel/Chaines/$1"
        },
        {
            "src": "/images/DungeonTravel/Porte/(.*)",
            "dest": "/public/images/DungeonTravel/Porte/$1"
        },
        {
            "src": "/images/DungeonTravel/Torche/(.*)",
            "dest": "/public/images/DungeonTravel/Torche/$1"
        },
        {
            "src": "/images/Neocity/(.*)",
            "dest": "/public/images/Neocity/$1"
        },
        {
            "src": "/images/Remindr/(.*)",
            "dest": "/public/images/Remindr/$1"
        },

        {
            "src": "/CV_Quentin_Lorenzo.pdf",
            "dest": "/public/CV_Quentin_Lorenzo.pdf"
        },
        {
            "src": "/languages.xml",
            "dest": "/public/languages.xml"
        },


        {
            "src": "/(.*)",
            "dest":"/api/index.php"
        }
    ]
}